@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <table>
            <tr>
                <td>客戶代號</td>
                <td>客戶名稱</td>
                <td>員工名稱</td>
            </tr>
                @foreach (var item in (List<homework.Models.Order>)ViewBag.Data)
                    {
                        <tr>
                            <td>@item.CustId</td>
                            <td>@item.CustName</td>
                            <td>@item.EmpName</td>
                        </tr>
                    }
        </table>
    </div>
</body>
</html>